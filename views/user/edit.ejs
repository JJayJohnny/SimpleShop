<form method="POST" action="/user/edit">
    <label>Email</label>
    <input type="text" name="email" required value="<%= user.email%>"/><br/>
    <h4>Address:</h4>
    <label>Street name</label>
    <input type="text" name="street" required value="<%= user.address.street%>"/><br/>
    <label>House number</label>
    <input type="text" name="houseNumber" required value="<%= user.address.houseNumber%>"/><br/>
    <label>City</label>
    <input type="text" name="city" required value="<%= user.address.city%>"/><br/>
    <label>Postal code</label>
    <input type="text" name="postalCode" required value="<%= user.address.postalCode%>"/><br/>
    <label>Country</label>
    <input type="text" name="country" required value="<%= user.address.country%>"/><br/>
    <input type="submit" value="Save"/>
</form>